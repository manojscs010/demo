import java.util.*;
class Plset2rev
{
    public static void main(String args[])
    {
       Scanner sc=new Scanner(System.in);
       int n=sc.nextInt();
       int i=0;
        String s=sc.next();
         String str="";
        for(i=n-1;i>=0;i--)
        {
            if(!(s.charAt(i)=='a')&&!(s.charAt(i)=='e')&&!(s.charAt(i)=='i')&&!(s.charAt(i)=='o')&&!(s.charAt(i)=='u'))
             str=str+s.charAt(i);
        }
        System.out.print(str);
    }
}
