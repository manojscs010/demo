import java.util.*;
class Plset2sq
{
    public static void main(String args[])
    {
       Scanner sc=new Scanner(System.in);
        String n=sc.next();
       char a[]=n.toCharArray();
       int sum=0;
        for(int i=0;i<a.length;i++)
        {
            sum=sum+Character.getNumericValue(a[i])*Character.getNumericValue(a[i]);
        }
        System.out.print(sum);
         
    }
}
